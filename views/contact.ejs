<%- include('./partials/head') %>

<main class="main-content">
  <section class="contact-form-container">
    <h1 class="title">Contact us</h1>
    <form
      class="contact-form"
      action="https://formshow.systementor.se"
      method="post"
    >
      <input
        class="form-name"
        type="text"
        name="name"
        placeholder="Your name"
      />
      <input class="form-email" type="email" name="email" placeholder="Email" />
      <input
        class="form-errand"
        type="text"
        name="errand"
        placeholder="Errand"
      />
      <textarea
        class="form-message"
        name="message"
        rows="10"
        placeholder="Message"
      ></textarea>
      <button class="form-submit">Submit</button>
    </form>
  </section>
</main>

<%- include('./partials/footer') %>
